<Window x:Class="Template_4333._4333_Sagitova"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Template_4333"
        mc:Ignorable="d"
        Title="_4333_Sagitova" Height="550" Width="500">
    <Grid Background="Lavender">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <Label Content="Сагитова Дина 4333 19 лет" FontSize="30" HorizontalAlignment="Center" Height="50" Grid.Row="0" ></Label>
        <Button Content="Импорт данных" Width="150" Height="30" HorizontalAlignment="Center" Click="Button_Click" Grid.Row="1"></Button>
        <Button Content="Экспорт в Excel" Width="150" Height="30" HorizontalAlignment="Center" Click="Button_Click_1" Grid.Row="2"></Button>
    </Grid>
</Window>
